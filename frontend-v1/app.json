{
  "expo": {
    "name": "Safe Rides",
    "privacy": "public",
    "description": "Safe Rides is a mobile application that helps ride hailers/users check whether their selected Bolt ride is safe or not before boarding a cab.",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "slug": "phelcia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "phelcia.ridefy",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#143843"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
